{
  "manifest_version": 2,
  "name": "Aprelendo",
  "version": "1.2",
  "description": "Add texts to your Aprelendo library.",
  "homepage_url": "https://www.aprelendo.com",
  "icons": {
    "48": "icons/aprelendo.svg",
    "96": "icons/aprelendo.svg"
  },
  
  "applications": {
    "gecko": {
      "id": "{d89b9665-3b30-4586-9bff-7b9aeea27145}"
    }
  },

  "permissions": [
    "activeTab",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icons/aprelendo.svg",
    "default_title": "Add to Aprelendo",
    "default_popup": "popup/choose_lang.html"
  },
  
  "options_ui": {
    "page": "options.html"
  }

}
